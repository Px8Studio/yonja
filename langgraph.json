{
    "$schema": "https://langchain-ai.github.io/langgraph/langgraph.json",
    "dependencies": ["."],
    "graphs": {
        "yonca_agent": "./src/yonca/agent/graph.py:create_agent_graph"
    },
    "env": {
        "YONCA_LLM_PROVIDER": "YONCA_LLM_PROVIDER",
        "YONCA_OLLAMA_BASE_URL": "YONCA_OLLAMA_BASE_URL",
